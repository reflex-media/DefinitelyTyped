{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "baseUrl": "../",
        "typeRoots": [
            "../"
        ],
        "paths": {
            "lesgo": [
                "lesgo/lib"
            ],
            "lesgo/*": [
                "lesgo/lib/*"
            ]
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "files": [
        "index.d.ts",
        "lesgo-tests.ts"
    ]
}
